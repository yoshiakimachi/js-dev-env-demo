    "babel-register": "6.26.0",
    "chai": "3.5.0",
    "chalk": "^2.3.0",
    "cheerio": "0.22.0",
    "cross-env": "2.0.0",
    "css-loader": "^0.28.9",
    "eslint": "3.4.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-watch": "2.1.14",
    "express": "^4.16.2",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.4.2",
    "localtunnel": "^1.8.3",
    "mocha": "3.0.2",
    "nock": "8.0.0",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "style-loader": "^0.20.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-md5-hash": "0.0.5"
